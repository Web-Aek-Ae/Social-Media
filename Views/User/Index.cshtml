@model IEnumerable<SocialMedia.Models.Database.User>

<h2>Users</h2>

<table>
    <thead>
        <tr>
            <th>UserId</th>
            <th>Name</th>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
            <!-- Add other fields as needed -->
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserId</td>
            <td>@user.Name</td>
            <td>@user.Username</td>
            <td>@user.Email</td>
            <td>@user.Password</td>
            <!-- Display other fields as needed -->
        </tr>
    }
    </tbody>
</table>
